apiVersion: networking.istio.io/v1beta1
kind: VirtualService
metadata:
  name: react-phase2-vs-v1
  namespace: react-phase2
spec:
  hosts:
  - "*"
  gateways:
  - react-gateway-phase2
  http:
  - route:
    - destination:
        host: react-frontend
        port:
          number: 80
      weight: 100

